{
  "name": "typescript-demo",
  "version": "1.0.0",
  "description": "Demonstration typescript project",
  "main": "main.ts",
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'tests/**/*.ts'",
    "test": "jest --verbose",
    "test:watch": "jest --verbose --watch"
  },
  "keywords": [
    "typescript"
  ],
  "author": "MikingTheViking",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^6.0.0",
    "reflect-metadata": "^0.1.12",
    "typescript": "^2.6.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^9.3.0",
    "@types/supertest": "^2.0.4",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "supertest": "^3.0.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.5.0",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.3.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
