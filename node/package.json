{
  "name": "typescript-demo",
  "version": "1.0.0",
  "description": "Demonstration typescript project",
  "main": "main.ts",
  "scripts": {
    "test": "jest",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "webpack": "webpack --config webpack.config.js",
    "build": "webpack",
    "build:watch": "webpack -w",
    "build:prod": "wepbpack -p"
  },
  "keywords": [
    "typescript"
  ],
  "author": "MikingTheViking",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^6.0.0",
    "typescript": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "jest": "^23.5.0",
    "mini-css-extract-plugin": "^0.4.1",
    "nodemon": "^1.18.3",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.5.0",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
